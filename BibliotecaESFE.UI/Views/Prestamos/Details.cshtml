@model BibliotecaESFE.EN.Prestamos

@{
    ViewData["Title"] = "Details";
}

<h1>Detalles</h1>

<div>
    <h4>Prestamos</h4>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">Id</dt>
        <dd class="col-sm-10">@Html.DisplayFor(model => model.Id)</dd>

        <dt class="col-sm-2">UsuarioId</dt>
        <dd class="col-sm-10">@Html.DisplayFor(model => model.UsuarioId)</dd>

        <dt class="col-sm-2">LibroId</dt>
        <dd class="col-sm-10">@Html.DisplayFor(model => model.LibroId)</dd>

        <dt class="col-sm-2">FechaInicio</dt>
        <dd class="col-sm-10">@Html.DisplayFor(model => model.FechaInicio)</dd>

        <dt class="col-sm-2">FechaVencimiento</dt>
        <dd class="col-sm-10">@Html.DisplayFor(model => model.FechaVencimiento)</dd>

        <dt class="col-sm-2">FechaDevolucion</dt>
        <dd class="col-sm-10">@Html.DisplayFor(model => model.FechaDevolucion)</dd>

        <dt class="col-sm-2">Top_Aux</dt>
        <dd class="col-sm-10">@Html.DisplayFor(model => model.Top_Aux)</dd>
    </dl>
</div>

<div>
    <a asp-action="Edit" asp-route-id="@Model.Id">Editar</a> |
    <a asp-action="Index">Regresar a la lista</a>
</div>
